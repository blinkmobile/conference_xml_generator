<tr class="fields">
  <td class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </td>
  <td>
        <table><tr></tr>
        <% f.fields_for :abstractnmaps do |builder| %>
            <%=render 'abstractnmap_fields', :f => builder %>
        <% end %>
        </table>
        <p><%= link_to_add_fields "Add Speaker", f, :abstractnmaps %></p>
  </td>
  <td class="field">
    <%= f.label :text, 'Abstract Text'%>
    <%= f.text_field :text %>
  </td>
    <td class="field">
        <%= f.hidden_field :_destroy %>
        <%= link_to_remove_fields "remove", f %>
    </td>
</tr>