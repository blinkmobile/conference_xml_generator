<h1>Listing groupings</h1>

<table>
  <tr>
    <th>Group</th>
    <th>Subgroup</th>
    <th>Session</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @groupings.each do |grouping| %>
  <tr>
    <td><%= grouping.group_id %></td>
    <td><%= grouping.subgroup_id %></td>
    <td><%= grouping.session_id %></td>
    <td><%= link_to 'Show', grouping %></td>
    <td><%= link_to 'Edit', edit_grouping_path(grouping) %></td>
    <td><%= link_to 'Destroy', grouping, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Grouping', new_grouping_path %>
