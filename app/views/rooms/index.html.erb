<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Imageurl</th>
    <th>Conference</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%= room.name %></td>
    <td><%= link_to 'Show', conference_room_path(@conference, room) %></td>
    <td><%= link_to 'Edit', edit_conference_room_path(@conference, room) %></td>
    <td><%= link_to 'Destroy', conference_room_path(@conference, room), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

