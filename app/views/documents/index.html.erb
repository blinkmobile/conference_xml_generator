<h1>Listing documents</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Conference</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @documents.each do |document| %>
  <tr>
    <td><%= document.title %></td>
    <td><%= document.url %></td>
    <td><%= document.conference_id %></td>
    <td><%= link_to 'Show', conference_document_path(@conference, document) %></td>
    <td><%= link_to 'Edit', edit_conference_document_path(@conference, document) %></td>
    <td><%= link_to 'Destroy', conference_document_path(@conference, document), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

