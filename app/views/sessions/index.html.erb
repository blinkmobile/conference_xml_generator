<h1>Listing sessions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Day</th>
    <th>Starttime</th>
    <th>Endtime</th>
    <th>Speaker</th>
    <th>Abstract</th>
    <th>Room</th>
    <th>Conference</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sessions.each do |session| %>
  <tr>
    <td><%= session.title %></td>
    <td><%= session.day_id %></td>
    <td><%= session.starttime %></td>
    <td><%= session.endtime %></td>
    <td><%= link_to 'Show', conference_session_path(@conference, session) %></td>
    <td><%= link_to 'Edit', edit_conference_session_path(@conference, session) %></td>
    <td><%= link_to 'Destroy', conference_session_path(@conference, session), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Session', new_conference_session_path %>
